<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_for @application, url: "/individual/steps/10", method: :post do |f| %>
      <%= f.govuk_collection_radio_buttons :dbs_check_type,
        @application.dbs_check_types.map{ |o| OpenStruct.new(id: o, name: o) },
        :id,
        :name,
        legend: { text: 'What type of living space can you offer?' }
      %>
      <%= f.govuk_submit t('continue') %>
    <% end %>
  </div>
</div>
